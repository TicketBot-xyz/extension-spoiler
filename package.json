		"rollup": "^4.9.6",
		"rollup-plugin-auto-external": "^2.0.0",
		"rollup-plugin-sourcemaps": "^0.6.3",
		"rollup-plugin-typescript2": "^0.36.0"
	},
	"peerDependencies": {
		"@tiptap/core": "^2.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ticketbot-zyx/extension-spoiler"
