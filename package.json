{
	"name": "@ticketbot/extension-spoiler",
	"description": "spoiler extension for tiptap",
	"version": "1.0.0",
	"homepage": "https://ticketbot.xyz",
	"keywords": [
		"tiptap",
		"tiptap extension"
	],
	"license": "MIT",
	"type": "module",
	"exports": {
